import java.io.*;
import java.util.*;
import java.util.Scanner;
import java.util.Arrays;   
public class CandidateCode {
    public static void main(String args[] ) throws Exception {

    	//Write code here
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=sc.nextInt();
        }
        Arrays.sort(a);  
      
          System.out.println(a[n-2]);
      
   }
}
