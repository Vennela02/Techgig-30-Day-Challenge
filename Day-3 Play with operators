
import java.io.*;
import java.util.*;
import java.util.Scanner;


public class CandidateCode {
    public static void main(String args[] ) throws Exception {

    	//Write code here
        Scanner sc=new Scanner(System.in);
       
        double a=sc.nextDouble();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int res=((int)a*b*c)/100;
        System.out.println(res);

   }
}
